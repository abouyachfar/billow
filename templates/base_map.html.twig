    <body>
        <!-- Start Secondary Menu-->
            <div class="secondary-menu">
                <ul class="menu-list">
                    <li><i class="fa fa-home" style="font-size: 23px;"></i> <a href="{{ path('home') }}" class="menu-link" style="padding: 12px 20px 12px 5px">Billow</a></li>
                    <li><i class="fa fa-mobile" style="font-size: 25px;"></i> <a style="padding: 12px 20px 12px 5px" href="tel: {{ project_tel }}" class="menu-link">{{ project_tel }}</a></li>
                    {% if(is_granted('ROLE_USER') == true and is_granted('ROLE_ADMIN') == false) %}
                        <li><i class="fa fa-plus-circle" style="font-size: 23px;"></i> <a style="padding: 5px;" class="menu-link" href="{{ path('myListing') }}"> {{ "My listings"|trans }}</a></li>
                    {% endif %}
                </ul>
            </div>
        <!-- End Secondary Menu -->

        <!-- Start Primary Menu-->
        <div id="primary-menu" class="primary-menu">
            <div class="logo-container">
                <a href="/{{ app.request.locale }}" title="Billow"><img src="/img/logo.png" class="logo-header"></a>
            </div>

            <nav class="nav-left navbar navbar-expand-lg">
                <div class="container-fluid">
                    <div class="collapse navbar-collapse" id="navbarNavDarkDropdown">
                        <ul class="navbar-nav">
                            <li class="nav-item dropdown">
                            <a class="menu-link nav-link dropdown-toggle" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                {{ 'Search'|trans }}
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDarkDropdownMenuLink">
                                <li><a href="{{ path('list_result') }}" class="sub-menu-link dropdown-item" href="#">{{ 'List'|trans }}</a></li>
                                <li><a href="{{ path('map') }}"class="sub-menu-link dropdown-item" href="#">{{ 'Map'|trans }}</a></li>
                            </ul>
                            </li>
                        </ul>

                        <ul class="navbar-nav">
                            <li class="nav-item dropdown">
                            <a class="menu-link nav-link dropdown-toggle" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                {{ 'Buy'|trans }}
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDarkDropdownMenuLink">
                                {% if buyPages is defined and buyPages is not empty %}
                                    {% for buy in buyPages %}
                                        <li><a class="sub-menu-link dropdown-item" href="/{{ app.request.locale }}/buy/{{ buy.id }}">{{ buy.title|trans }}</a></li>
                                    {% endfor %}
                                {% endif %}
                            </ul>
                            </li>
                        </ul>

                        <ul class="navbar-nav">
                            <li class="nav-item dropdown">
                            <a class="menu-link nav-link dropdown-toggle" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                {{ 'Sell'|trans }}
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDarkDropdownMenuLink">
                                {% if sellPages is defined and sellPages is not empty %}
                                    {% for sell in sellPages %}
                                        <li><a class="sub-menu-link dropdown-item" href="/{{ app.request.locale }}/sell/{{ sell.id }}">{{ sell.title|trans }}</a></li>
                                    {% endfor %}
                                {% endif %}
                            </ul>
                            </li>
                        </ul>

                        <ul class="navbar-nav">
                            <li><a href="{{ path('faq') }}" class="menu-link">{{ 'FAQ'|trans }}</a></li>
                            <li><a href="{{ path('pricing') }}" class="menu-link">{{ 'Pricing'|trans }}</a></li>
                        </ul>
                    </div>
                </div>
            </nav>

            <div class="nav-right">
                <ul class="menu-list">
                    <li><a href="{{ path('contact') }}" class="menu-link">{{ 'Contact'|trans }}</a></li>
                    {% if(app.request.locale != "en") %}
                        <li><a href="/en" title="English" class="menu-link">EN</a></li>
                    {%  else  %}
                        <li><a href="/cn" title="Chinese" class="menu-link">CN</a></li>
                    {% endif %}
                    
                    {% if(is_granted('ROLE_USER') == true and is_granted('ROLE_ADMIN') == false) %}
                        {% if(app.user.firstname != "") %}
                            <li><a id="btn-user" class="btn btn-connexion">{{ app.user.firstname }} <i class="fa fa-user-circle"></i></a></li>
                        {% else %}
                            <li><a id="btn-user" class="btn btn-connexion">{{ app.user.username }} <i class="fa fa-user-circle"></i></a></li>
                        {% endif %}

                        <div class="user-links hidden">
                            {% if(app.user.firstname != "") %}
                                <p class="title">{{ app.user.firstname }} {{ app.user.lastname }}</p>
                            {% else %}
                                <p class="title">{{ app.user.username }}</p>
                            {% endif %}
                            
                            <a href="{{ path('seller_account') }}"><i class="fa fa-home"></i> <span>{{ "My seller account"|trans }}</span></a>
                            <a href="{{ path('myListing') }}"><i class="fa fa-plus-circle"></i> <span>{{ "Add a new listing"|trans }}</span></a>
                            <hr/>
                            <!-- <a href=""><i class="fa fa-search"></i> <span>{{ "My buyer account"|trans }}</span></a> -->
                            <a href="{{ path('my_favorites') }}"><i class="fa fa-heart"></i> <span>{{ "My favorits"|trans }}</span></a>
                            <a href="{{ path('my_alert') }}}"><i class="fa fa-envelope-open"></i> <span>{{ "My alerts"|trans }}</span></a>
                            <hr/>
                            <a href="{{ path('account_settings') }}"><i class="fa fa-cog"></i> <span>{{ "Settings"|trans }}</span></a>
                            <hr/>
                            <a href="{{ path('app_logout') }}"><i class="fa fa-power-off"></i> <span>{{ "Logout"|trans }}</span></a>
                        </div>
                    {% else %}
                        {% if is_granted('ROLE_ADMIN') %}
                            <li><a href="{{ path('admin') }}" class="btn btn-connexion">{{ 'Billow Admin'|trans }} <i class="fa fa-user-circle"></i></a></li>
                        {% else %}
                            <li><a href="{{ path('app_login') }}" class="btn btn-connexion">{{ 'Login'|trans }} <i class="fa fa-user-circle"></i></a></li>
                        {% endif %}
                    {% endif %}
                </ul>
            </div>
        </div>
        <!-- End Primary Menu-->

        <!-- Start Mobile Menu -->
        <div class="topnav">
            <a href="#home" class="active">{{ 'Menu'|trans }}</a>
