        {% block javascripts %}
            <!--Plugin JavaScript file-->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/ion-rangeslider/2.3.1/js/ion.rangeSlider.min.js"></script>

            <!-- Main Script -->
            <script src="/js/functions.js"></script>
            <script src="/js/main.js"></script>

            <!--scripts loaded here-->
            
            <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
            <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
            <script src="/js/carousel.js"></script>

            <script>
                var $range_price = $("#demo_1").ionRangeSlider({
                    type: "double",
                    min: 0,
                    max: 5000000,
                    to: 5000000,
                    from: 0,
                    skin: "round",
                    prefix: "$",
                    step: 5000,
                    max_postfix: "+",
                });

                $range_price.on("change", function () {
                    var $inp = $(this);
                    var from = Intl.NumberFormat().format($inp.data("from")); // reading input data-from attribute
                    var to = Intl.NumberFormat().format($inp.data("to"));

                    if ($("#range-price-lbl").find(".cretaria-text").length) {
                        $("#range-price-lbl").find(".cretaria-text").html("$<span id='price_min'>" + from + "</span> - $<span id='price_max'>" + to + "</span>");
                        $("#range-price-lbl").removeClass("hidden");
                        $(".cretaria-choice").removeClass("hidden");
                    } else {
                        var _html = '<div id="range-price-lbl"><div class="cretaria-text">$<span id="price_min">' + from + '</span> - $<span id="price_max">' + to + '</span></div><div class="cretaria-remove">X</div></div>';
                        $("#cretaria-choices").prepend(_html);
                        $(".cretaria-choice").removeClass("hidden");
                    }
                });
            </script>

            <script>var markers = [];</script>

            {% if properties|length > 0 %}
                {% for property in properties %}
                    <script>
                        var id = {{ property.id }};
                        var latitude = {{ property.latitude }};
                        var longitude = {{ property.longitude }};
                        var price = {{ property.price }};
                        var city = "{{ property.city.label }}";
                        var street = "{{ property.street }}";
                        var img = "";
                        
                        {% if property.pictures is not empty %}
                            img = "{{ property.pictures[0].url }}";
                        {% endif %}
                        
                        markers.push([latitude, longitude, price, city, street, img, id]);
                    </script>
                {% endfor %}
            {% endif %}

            <script>
                jQuery(document).ready(function(){
                    // Default View
                    var view = [56.130366, -106.346771];

                    // Set first marker as view
                    if (markers.length > 0) {
                        view = [markers[0][0], markers[0][1]];
                    }
                    
                    // Creat map and set view
                    var map = L.map('map').setView(view, 6);

                    // Set tileLayer
                    L.tileLayer('https://api.mapbox.com/styles/v1/{id}/tiles/{z}/{x}/{y}?access_token={accessToken}', {
                        attribution: 'Map data &copy; <a href="https://www.openstreetmap.org/copyright">OpenStreetMap</a> contributors, Imagery © <a href="https://www.mapbox.com/">Mapbox</a>',
                        maxZoom: 18,
                        id: 'mapbox/streets-v11',
                        tileSize: 512,
                        zoomOffset: -1,
                        accessToken: 'sk.eyJ1IjoiYWJvdXlhY2hmYXIiLCJhIjoiY2t6azIzc2k0MGM5ZjJ1bnowb3dod3d4YiJ9.GU0AVkLTkYscMAf_PBOq_Q'
                    }).addTo(map);

                    // Set Markers
                    var marker = [];
                    var i;
                    for (i=0; i<markers.length; i++) {
                        if(markers[i][0] != "" && markers[i][1] != ""
                            && markers[i][0] != undefined && markers[i][1] != undefined) {
                                _html = '<div class="map-desc">';
                                _html += '<div class="map-img">';
                                _html += '<img src="' + markers[i][5] + '"/>';
                                _html += '</div>';

                                _html += '<div class="map-desc-text">';
                                _html += '<p class="price">$' + markers[i][2] + '</p>';
                                _html += '<p class="city">' + markers[i][3] + '</p>';
                                _html += '<p class="street">' + markers[i][4] + '</p>';
                                _html += '</div>';
                                _html += '</div>';

                                marker[i] = new L.Marker([markers[i][0], markers[i][1]], {
                                    win_url: "/{{ app.request.locale }}/details/" + markers[i][6] + "/" + "{{ page_name }}"
                                });
                                marker[i].bindTooltip(_html).openTooltip().addTo(map);
                                marker[i].on('click', onClick);
                        }
                    }
                });

                function onClick(e) {
                    window.location = this.options.win_url;
                }
            </script>

            <!-- Translation words on js -->
            <script>
                $.get("http://billow.ca/messages/{{ app.request.locale }}.json", function(data){
                    transMessages = data;
                });
            </script>
            <script language="javascript" src="/js/GetText.js"></script>
        {% endblock %}
